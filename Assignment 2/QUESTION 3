#include <iostream>
using namespace std;

int Linear(int arr[], int len, int n) {
    if (len == 0 || arr[0] != 1) return 1;   
        for (int i = 0; i < len; i++) 
        {
        int expected = i + 1;
        if (arr[i] != expected) return expected;
    }
    return n;  
}


int Binary(int arr[], int len, int n) {
    if (len == 0 || arr[0] != 1) return ;

    int low = 0, high = len - 1;
    while (low <= high) {
        int mid = (low + high) / 2;
        int expected = mid + 1;
        if (arr[mid] == expected) {
            low = mid + 1;  
        } else {
            high = mid - 1;  
        }
    }
    return low + 1;   
}

int main() {
    
    int arr1[] = {1, 2, 3, 5, 6, 7, 8};  
    int n = 8;
    int len = 7;
    cout << "Linear : " << missingLinear(arr, len, n) << endl;
    cout << "Binary : " << missingBinarySearch(arr, len, n) << endl;


    return 0;
}
