#include<iostream>
#include<queue>
using namespace std;

int main()
{

queue<int> queue1;
queue<int> queue2;
for(int i=0;i<6;i++)
{
 int x=i+1;
 queue1.push(x);
}

int i=0;
while(i<3)
{
 int x=queue1.front();
 queue1.pop();
 queue2.push(x);
 i++;
}

while(!queue2.empty())
{
 int z=queue1.front();
 queue1.pop();
 queue1.push(z);
 int q=queue2.front();
 queue2.pop();
 queue1.push(q);
}

while(!queue1.empty())
{
 cout<<queue1.front()<<"\t";
 queue1.pop();
}
}
